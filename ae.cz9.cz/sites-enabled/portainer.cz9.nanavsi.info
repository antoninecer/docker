server {

	server_name portainer.cz9.nanavsi.info portainer.cz9.cz;

	location / {
	    proxy_set_header Host $host;
	    proxy_set_header X-Real-IP $remote_addr;
	    proxy_pass http://127.0.0.1:9000;
	}


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/portainer.cz9.cz/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/portainer.cz9.cz/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = portainer.cz9.nanavsi.info) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

    if ($host = portainer.cz9.cz) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

	listen 80;

	server_name portainer.cz9.nanavsi.info portainer.cz9.cz;
    return 404; # managed by Certbot


}
